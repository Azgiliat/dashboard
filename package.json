{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint:eslint": "yarn eslint --cache --max-warnings 0 --report-unused-disable-directives \"**/*.{js,ts,tsx,vue,html}\"",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "yarn prettier --check \"**/*.{js,json,ts,vue,html}\"",
    "lint:prettier:fix": "yarn prettier --write \"**/*.{js,json,ts,vue,html}\"",
    "lint:stylelint": "yarn stylelint --cache \"**/*.{html,css,vue}\"",
    "lint:stylelint:fix": "yarn lint:stylelint --fix",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:stylelint",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:prettier:fix && yarn lint:stylelint:fix"
  },
  "dependencies": {
    "firebase": "^11.3.1",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@tailwindcss/vite": "^4.0.7",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/eslint__js": "^8.42.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^16.0.0",
    "nanoid": "^5.1.0",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "stylelint": "^16.7.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^36.0.1",
    "tailwindcss": "latest",
    "typescript": "latest",
    "typescript-eslint": "latest",
    "vite": "6.1.1",
    "vite-plugin-svg-icons": "2.0.1",
    "vue-eslint-parser": "9.4.3",
    "vue-tsc": "2.0.26"
  }
}
