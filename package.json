{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint:eslint": "yarn eslint --cache --max-warnings 0 --report-unused-disable-directives \"**/*.{js,ts,tsx,vue,html}\"",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "yarn prettier --check \"**/*.{js,json,ts,vue,html}\"",
    "lint:prettier:fix": "yarn prettier --write \"**/*.{js,json,ts,vue,html}\"",
    "lint:stylelint": "yarn stylelint --cache \"**/*.{html,css,vue}\"",
    "lint:stylelint:fix": "yarn lint:stylelint --fix",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:stylelint",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:prettier:fix && yarn lint:stylelint:fix"
  },
  "dependencies": {
    "firebase": "^9.17.2",
    "pinia": "^2.0.33",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.9.0",
    "nanoid": "^4.0.1",
    "postcss": "^8.4.12",
    "postcss-html": "1.5.0",
    "prettier": "2.8.4",
    "stylelint": "15.1.0",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "30.0.1",
    "stylelint-order": "6.0.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.24"
  }
}
